{
    "name": "fbf/laravel-pages",
    "description": "A Laravel 4 package for adding simple content managed pages to a website with banner image, heading, main image or YouTube video and body content.",
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "cviebrock/eloquent-sluggable": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Fbf\\LaravelPages\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
